{
  "name": "stakei",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "db:generate": "pnpm --filter @stakei/db db:generate",
    "db:migrate": "pnpm --filter @stakei/db db:migrate",
    "db:push": "pnpm --filter @stakei/db db:push",
    "db:studio": "pnpm --filter @stakei/db db:studio"
  },
  "devDependencies": {
    "turbo": "^2"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild", "sharp", "unrs-resolver"]
  }
}
